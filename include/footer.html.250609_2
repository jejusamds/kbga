	var m_sub_banner_sub_nav_slide = null;
	<?php } ?>

	// 화면 리사이징
	$(window).resize(function() {
		// 현재 window 너비
		var currentWidth = window.innerWidth;

		// 너비가 변경되지 않은 경우(스크롤, 상태바 동작) 무시
		if (currentWidth === basic_initialWidth && !basic_isFirstLoad) {
			return;
		}

		clearTimeout(basic_resizeTimer);
		
		// 화면 너비
		if (window.innerWidth > 1024) {
			// 모바일 메인 메뉴 예외처리
			if( $("#m_main_nav").css("display") != "none" ){
				$("body").removeClass("hidden");
				$("#header").removeClass("on");
				$("#m_main_nav_bg").hide();
				$("#m_main_nav").hide();
			}
			
			// 모바일 메인 메뉴 검색창 예외처리
			$("#m_main_nav > .contents_con > .list_con > .search_con .input_con > table > tbody > tr > .input_td .input").val("");
		}else{
			// 헤더 예외처리
			if( $("#header > .sub_nav").css("display") != "none" ){
				$("#header").removeClass("over");
				$("#header > .sub_nav").hide();
			}
			
			// 헤더 검색창 예외처리
			$("#header > .contents_con > table > tbody > tr > .sm_td > .search_con .input_con > table > tbody > tr > .input_td .input").val("");

			<?php if ($ssMenu_slide != "") { ?>
			// 서브 배너 서브 메뉴 슬라이드 예외처리
			$("#sub_con > .banner_con > .sub_nav > .slide_con").css({"opacity":"0","transition":"none"});
			<?php } ?>
		}

		basic_resizeTimer = setTimeout(function() {
			// 실제 리사이징이 발생한 경우에만 처리
			if (currentWidth !== basic_initialWidth || basic_isFirstLoad) {
				// 화면 너비
				if (window.innerWidth > 1024) {
					// 헤더 오버 시
					$("#header").hover(
						function(){
							// 진행 중인 애니메이션 정지하고 큐 비우기
							$("#header > .sub_nav").stop(true, false);
							$("#header").addClass("over");
							$("#header > .sub_nav").slideDown(500);
						},
						function(){
							// 진행 중인 애니메이션 정지하고 큐 비우기
							$("#header > .sub_nav").stop(true, false);
							$("#header > .sub_nav").slideUp(500, function(){
								$("#header").removeClass("over");
							});
						}
					);

					// 연합회소개 오버 시
					$("#header .a01").hover(
						function(){
							$("#header > .contents_con > table > tbody > tr > .nav_td > .list_con > ul > li > a.a01").addClass("over");
						},
						function(){
							$("#header > .contents_con > table > tbody > tr > .nav_td > .list_con > ul > li > a.a01").removeClass("over");
						}
					);

					// 협회소식 오버 시
					$("#header .a02").hover(
						function(){
							$("#header > .contents_con > table > tbody > tr > .nav_td > .list_con > ul > li > a.a02").addClass("over");
						},
						function(){
							$("#header > .contents_con > table > tbody > tr > .nav_td > .list_con > ul > li > a.a02").removeClass("over");
						}
					);

					// 자격시험센터 오버 시
					$("#header .a03").hover(
						function(){
							$("#header > .contents_con > table > tbody > tr > .nav_td > .list_con > ul > li > a.a03").addClass("over");
						},
						function(){
							$("#header > .contents_con > table > tbody > tr > .nav_td > .list_con > ul > li > a.a03").removeClass("over");
						}
					);

					// COMPETITION 오버 시
					$("#header .a04").hover(
						function(){
							$("#header > .contents_con > table > tbody > tr > .nav_td > .list_con > ul > li > a.a04").addClass("over");
						},
						function(){
							$("#header > .contents_con > table > tbody > tr > .nav_td > .list_con > ul > li > a.a04").removeClass("over");
						}
					);

					// 커뮤니티 오버 시
					$("#header .a05").hover(
						function(){
							$("#header > .contents_con > table > tbody > tr > .nav_td > .list_con > ul > li > a.a05").addClass("over");
						},
						function(){
							$("#header > .contents_con > table > tbody > tr > .nav_td > .list_con > ul > li > a.a05").removeClass("over");
						}
					);
				}else{
					// 헤더 오버 시
					$("#header").mouseenter(function(){
						$("#header").removeClass("over");
						$("#header > .sub_nav").stop().hide();	
					});

					<?php if ($ssMenu_slide != "") { ?>
					// 서브 배너 서브 메뉴 슬라이드 파괴
					if (m_sub_banner_sub_nav_slide) {
						m_sub_banner_sub_nav_slide.destroy();
						m_sub_banner_sub_nav_slide = null;
					}

					// 서브 배너 서브 메뉴 슬라이드
					m_sub_banner_sub_nav_slide = new Swiper ('.m_sub_banner_sub_nav_slide', {
						slidesPerView: 'auto',
						speed: 1500,
						loop: false,
						observer: true,
						observeParents: true,
					});
					
					// 모바일 서브 배너 서브 메뉴 슬라이드 gotoslide
					m_sub_banner_sub_nav_slide.slideTo(<?=$ssMenu_slide?>, 0);

					// 서브 배너 서브 메뉴 슬라이드 예외처리
					$("#sub_con > .banner_con > .sub_nav > .slide_con").css({"opacity":"1","transition":"all 0.5s ease"});
					<?php } ?>

