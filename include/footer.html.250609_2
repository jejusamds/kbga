	// 모바일 메인 메뉴
	function m_main_nav(){
		var effect = 'slide';
		var options = { direction: "right" };
		var options1 = { direction: "left" };
		var duration = 500;
		if($("#m_main_nav").css("display") == "none") {
			$("#m_main_nav > .contents_con > .list_con").css("padding",($("#header").innerHeight()*2)+"px 0px "+$("#header").innerHeight()+"px 0px");
			$("#m_main_nav > .contents_con > .list_con > .scroll_con").css("height",($(window).innerHeight() - ($("#header").innerHeight()*3))+"px").scrollTop(0);
			$("body").addClass("hidden");
			$("#header").addClass("on");
			$("#m_main_nav_bg").fadeIn("500");
			$("#m_main_nav").toggle(effect, options, duration);
		}
		else {
			$("body").removeClass("hidden");
			$("#header").removeClass("on");
			$("#m_main_nav_bg").fadeOut("500");
			$("#m_main_nav").toggle(effect, options, duration);
		}
	}

	// 모바일 메인 메뉴 하위 메뉴 클릭 시
	$(document).on("click","#m_main_nav > .contents_con > .list_con > .scroll_con > .contents_con > ul > li > a",function(){
		$("#m_main_nav > .contents_con > .list_con > .scroll_con > .contents_con > ul > li > a").each(function(){
			$(this).removeClass("on");
			$(this).next(".sub_nav").slideUp();
		});

		if( $(this).next(".sub_nav").css("display") == "none" ){
			$(this).addClass("on");
			$(this).next(".sub_nav").slideDown();
		}else{
			$(this).removeClass("on");
			$(this).next(".sub_nav").slideUp();
		}
	});

	// 스크롤 이벤트
	$(window).on("resize scroll", function() {
		if ($(this).scrollTop() > 0) {
			$("#header").addClass("scroll");
		} else {
			$("#header").removeClass("scroll");
		}
	});

	// 초기 화면 크기 저장
	var basic_initialWidth = window.innerWidth;
	var basic_isFirstLoad = true;

	// 리사이즈 예외처리
	var basic_resizeTimer;
	
