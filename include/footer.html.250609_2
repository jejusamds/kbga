					}

					// 초기 상태 확인
					checkOrientation();

					// 회전 이벤트 감지
					window.addEventListener("resize", checkOrientation);
				}

				basic_initialWidth = currentWidth;
				basic_isFirstLoad = false;
			}
		}, 500);
	});
