/* vw 계산 기준 1920px */

@import url(//cdn.design-factory.co.kr/_font/onemobilepopotf/fonts.css);
@import url(//cdn.design-factory.co.kr/_font/pretendard/fonts.css);

* {font-style:normal; line-height:normal; font-family:"Pretendard", sans-serif; box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;}

/*
	font-family:"ONE Mobile POP OTF", sans-serif;
	font-family:"Pretendard", sans-serif;
*/

body {width:100%; margin:0 auto; padding:0; font-size:13px; line-height:normal; overflow-y:scroll;}
body.hidden {height:100%; min-height:100%; overflow:hidden !important; touch-action:none;}
img {border:none; padding:0; margin:0; outline:none;}
a {border:none; padding:0; margin:0; outline:none; text-decoration:none;}
a:active {color:#666666; text-decoration:none;}
a:link {color:#666666; text-decoration:none;}
a:visited {color:#666666; text-decoration:none;}
a:hover {color:#666666; text-decoration:none;}
ul {list-style:none; padding:0; margin:0;}
ul:after {content:""; display:block; clear:both;}
ul > li {display:block;}

img.fx{max-width:100%;}

td {
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
}

input, select, textarea {
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
	margin:0; 
	padding:0; 
	outline:none; 
	font-size:25px;
	color:#6B6B6B;
	letter-spacing:-0.6px;
}

input::placeholder {color:rgba(175,175,175,0.80); opacity:1;}
input::-webkit-input-placeholder {color:rgba(175,175,175,0.80);}
input:-ms-input-placeholder {color:rgba(175,175,175,0.80);}
input:-moz-input-placeholder {color:rgba(175,175,175,0.80);}

textarea {overflow-y:scroll; resize:none;}

textarea::placeholder {color:rgba(175,175,175,0.80); opacity:1;}
textarea::-webkit-input-placeholder {color:rgba(175,175,175,0.80);}
textarea:-ms-input-placeholder {color:rgba(175,175,175,0.80);}
textarea:-moz-input-placeholder {color:rgba(175,175,175,0.80);}

input[type=text], input[type=password] ,input[type=tel], input[type=file], input[type=image], input[type=button], input[type=submit], textarea {
	border-radius:0; 
	-moz-border-radius:0; 
	-webkit-border-radius:0;
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	width:100%;
}

select {
	border-radius:0; 
	-moz-border-radius:0; 
	-webkit-border-radius:0;
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	background-color:#ffffff;
	background-image:url("/df/kbga/img/sub/select_arrow.svg");
	background-repeat:no-repeat;
	background-size:17px; 
	background-position:right 25px center;
	width:100%;
	padding:0 67px 0 25px;
	color:#6B6B6B;
}

select::-ms-expand {display:none;}

.w_con {}
.m_con {display:none;}

.w_img {display:inline-block; width:100%;}
.m_img {display:none;}

.w_br {}
.m_br {display:none;}

.w_bar {}
.m_bar {display:none;}

/*체크박스 버튼*/
.checkbox_label {display:inline-block; cursor:pointer;}
.checkbox_label input[type=checkbox] {display:none;}
.checkbox_label .check_icon {display:inline-block; width:30px; height:30px; background-image:url("/df/kbga/img/sub/checkbox_icon_off.svg"); background-repeat:no-repeat; background-size:30px; margin:0 2px 0 0; vertical-align:middle;}
.checkbox_label span {vertical-align:middle;}
.checkbox_label input[type=checkbox]:checked + .check_icon {background-image:url("/df/kbga/img/sub/checkbox_icon_on.svg");}

/*라디오 버튼*/
.radio_label {display:inline-block; cursor:pointer;}
.radio_label input[type=radio] {display:none;}
.radio_label .check_icon {display:inline-block; width:18px; height:18px; background-image:url("/df/kbga/img/sub/radio_icon_off.svg"); background-repeat:no-repeat; background-size:18px; margin:0 2px 0 0; vertical-align:middle;}
.radio_label span {vertical-align:middle;}
.radio_label input[type=radio]:checked + .check_icon {background-image:url("/df/kbga/img/sub/radio_icon_on.svg");}

/*번호목록*/
.number_list_con {text-align:center;}
.number_list_con > .contents_con {display:inline-block;}
.number_list_con > .contents_con:after {content:""; display:block; clear:both;}
.number_list_con > .contents_con a {display:inline-block;}
.number_list_con > .contents_con .btn_con {display:inline-block; vertical-align:middle;}
.number_list_con > .contents_con .btn_con a {margin:0 0 0 12px;}
.number_list_con > .contents_con .btn_con a img {width:auto; max-width:none; height:16px;}
.number_list_con > .contents_con .btn_con a:first-child {margin:0;}
.number_list_con > .contents_con .list_con {display:inline-block; margin:0 17px; position:relative; top:0; left:0; vertical-align:middle;}
.number_list_con > .contents_con .list_con:after {content:""; display:block; clear:both;}
.number_list_con > .contents_con .list_con .list_a {margin:0 0 0 26px; font-size:18px; color:rgba(5,5,5,0.5); font-weight:700; line-height:110%; letter-spacing:-0.44px;}
.number_list_con > .contents_con .list_con .list_a:first-child {margin:0; color:#050505;}
.number_list_con > .contents_con .list_con .bar {width:2px; height:20px; margin:0 auto; background-color:rgba(5,5,5,0.5); position:absolute; top:50%; left:0; right:0; z-index:10; transform:translateY(-50%);}

#wrap {width:100%; min-width:280px; margin:0 auto;}

/*헤더*/
