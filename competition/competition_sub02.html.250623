	<script type="text/javascript" language="javascript">
		// 모바일 대회 게시판 리스트 열기/닫기
		$(document).on("click",".tournament_notice_con > ul > li .list_div > .btn_con.m_con .a_btn",function(){
			$(".tournament_notice_con > ul > li .list_div > .btn_con.m_con .a_btn").each(function(){
				$(this).closest(".list_div").find(".text_con.con").slideUp();
				$(this).find("img").css("transform","rotate(0deg)");
				$(this).find("span").html("상세보기");
			});

			if( $(this).closest(".list_div").find(".text_con.con").css("display") == "none" ){
				$(this).closest(".list_div").find(".text_con.con").slideDown();
				$(this).find("img").css("transform","rotate(180deg)");
				$(this).find("span").html("접기");
			}else{
				$(this).closest(".list_div").find(".text_con.con").slideUp();
				$(this).find("img").css("transform","rotate(0deg)");
				$(this).find("span").html("상세보기");
			}
		});
	
		// 초기 화면 크기 저장
		var competition_sub02_initialWidth = window.innerWidth;
		var competition_sub02_isFirstLoad = true;

		// 리사이즈 예외처리
		var competition_sub02_resizeTimer;

		// 화면 리사이징
		$(window).resize(function(){
			// 현재 window 너비
			var currentWidth = window.innerWidth;
			
			// 너비가 변경되지 않은 경우(스크롤, 상태바 동작) 무시
			if(currentWidth === competition_sub02_initialWidth && !competition_sub02_isFirstLoad) {
				return;
			}
			
			clearTimeout(competition_sub02_resizeTimer);
			
			// 대회 게시판 리스트 예외처리
			$(".tournament_notice_con > ul > li .list_div > .btn_con.m_con .a_btn > img").css("transform","rotate(0deg)");
			$(".tournament_notice_con > ul > li .list_div > .btn_con.m_con .a_btn > span").html("상세보기");

			// 화면 너비
			if (window.innerWidth > 1024) {
				// 대회 게시판 리스트 예외처리
				$(".tournament_notice_con > ul > li .list_div > .contents_con > .text_con").css("display","block");
			}else{
				// 대회 게시판 리스트 예외처리
				$(".tournament_notice_con > ul > li .list_div > .contents_con > .text_con").css("display","none");
			}

			competition_sub02_resizeTimer = setTimeout(function() {
				// 실제 리사이징이 발생한 경우에만 처리
				if(currentWidth !== competition_sub02_initialWidth || competition_sub02_isFirstLoad) {
					// 대회 게시판 리스트 예외처리
					$(".tournament_notice_con > ul > li .list_div > .btn_con.m_con .a_btn > img").css("transform","rotate(0deg)");
					$(".tournament_notice_con > ul > li .list_div > .btn_con.m_con .a_btn > span").html("상세보기");

					// 화면 너비
					if (window.innerWidth > 1024) {
						// 대회 게시판 리스트 예외처리
						$(".tournament_notice_con > ul > li .list_div > .contents_con > .text_con").css("display","block");
					}else{
						// 대회 게시판 리스트 예외처리
						$(".tournament_notice_con > ul > li .list_div > .contents_con > .text_con").css("display","none");
					}

					competition_sub02_initialWidth = currentWidth;
					competition_sub02_isFirstLoad = false;
				}
			}, 500);
		});
	</script>

